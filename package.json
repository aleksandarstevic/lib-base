{
  "name": "@aleksandarstevic/lib-base",
  "version": "1.0.0",
  "homepage": "https://github.com/aleksandarstevic/lib-basese#readme",
  "description": "Base configuration for lib",
  "bin": {
    "lib-base": "./bin/lib-base.sh"
  },
  "files": [
    "bin",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksandarstevic/lib-basese.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aleksandarstevic/lib-basese/issues"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "regenerator-runtime": "^0.12.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/tests/**/**/*.{js,jsx,mjs}",
      "<rootDir>/tests/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleDirectories": [
      "node_modules",
      "./src/"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "testPathIgnorePatterns": [
      "/tests/__mocks__/"
    ]
  },
  "devDependencies": {
    "jest": "^23.5.0"
  }
}
